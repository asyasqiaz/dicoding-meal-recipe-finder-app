{
  "env": {
    "es2021": true,
    "browser": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [0, { "js": "always" }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
